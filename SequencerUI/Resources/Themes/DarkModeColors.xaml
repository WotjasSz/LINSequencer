<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Colors-->
    <Color x:Key="Foreground_1_Color">#f7f7f7</Color>
    <Color x:Key="Foreground_2_Color">#c3c3c3</Color>
    
    <Color x:Key="ColorPrimary1_Light">#FFF0FFFF</Color>
    <Color x:Key="ColorPrimary2_Light">#FFecfbfb</Color>
    <Color x:Key="ColorPrimary3_Middle">#FFe7f5f5</Color>
    <Color x:Key="ColorPrimary4_Dark">#FF333333</Color>
    <Color x:Key="ColorSecondary1_DodgerBlueLight">#FFb3d9ff</Color>
    <Color x:Key="ColorSecondary2_DodgerBlue">#FF1E90FF</Color>
    <Color x:Key="ColorSecondary3_DodgerBlueDark">#FF004080</Color>
    
    <!--#endregion-->
    
    <!--#region SolidBrushColors -->
    <SolidColorBrush x:Key="Foreground_1" Color="{StaticResource Foreground_1_Color}"/>
    <SolidColorBrush x:Key="Foreground_2" Color="{StaticResource Foreground_2_Color}"/>
    <!--#endregion-->

</ResourceDictionary>
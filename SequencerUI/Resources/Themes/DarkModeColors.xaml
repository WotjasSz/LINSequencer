<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Colors-->
    <Color x:Key="Background_1_Color">#191A19</Color>
    <Color x:Key="Background_2_Color">#282A28</Color>
    <Color x:Key="Background_3_Color">#323432</Color>
    <Color x:Key="Background_4_Color">#464946</Color>
    <Color x:Key="Background_5_Color">#5A5E5A</Color>

    <Color x:Key="Foreground_1_Color">#FFFFFF</Color>
    <Color x:Key="Foreground_2_Color">#F5F5F5</Color>
    <Color x:Key="Foreground_3_Color">#EBEBEB</Color>
    <Color x:Key="Foreground_4_Color">#E0E0E0</Color>
    <Color x:Key="Foreground_5_Color">#B8B8B8</Color>

    <Color x:Key="Primary_1_Color">#1E5128</Color>
    <Color x:Key="Primary_2_Color">#276834</Color>
    <Color x:Key="Primary_3_Color">#2D763C</Color>
    <Color x:Key="Primary_4_Color">#38944A</Color>
    <Color x:Key="Primary_5_Color">#3EA352</Color>

    <Color x:Key="Secondary_1_Color">#4E9F3D</Color>
    <Color x:Key="Secondary_2_Color">#4E9F3D</Color>
    <Color x:Key="Secondary_3_Color">#60BC4E</Color>
    <Color x:Key="Secondary_4_Color">#7AC76B</Color>
    <Color x:Key="Secondary_5_Color">#7AC76B</Color>

    <Color x:Key="Accent_1_Color">#CCE28D</Color>
    <Color x:Key="Accent_2_Color">#D3E69E</Color>
    <Color x:Key="Accent_3_Color">#DAEAAE</Color>
    <Color x:Key="Accent_4_Color">#E9F2CE</Color>
    <Color x:Key="Accent_5_Color">#F1F6DF</Color>

    <Color x:Key="Scroll_1_Color">#B8B8B8</Color>

    <Color x:Key="GroupContentBackground_1_Color">#0FF5F5F5</Color>
    <Color x:Key="GroupContentBackground_2_Color">#0FEBEBEB</Color>

    <Color x:Key="ListItemSeparator_1_Color">#50F5F5F5</Color>
    <Color x:Key="ListItemSeparator_2_Color">#965A5E5A</Color>

    <Color x:Key="PopUp_1_Color">#F0A3AAA3</Color>
    <Color x:Key="PopUp_2_Color">#FFC7D0C7</Color>

    <!--#endregion-->

    <!--#region SolidBrushColors-->
    <SolidColorBrush x:Key="Background_1" Color="{StaticResource Background_1_Color}" />
    <SolidColorBrush x:Key="Background_2" Color="{StaticResource Background_2_Color}" />
    <SolidColorBrush x:Key="Background_3" Color="{StaticResource Background_3_Color}" />
    <SolidColorBrush x:Key="Background_4" Color="{StaticResource Background_4_Color}" />
    <SolidColorBrush x:Key="Background_5" Color="{StaticResource Background_5_Color}" />

    <SolidColorBrush x:Key="Foreground_1" Color="{StaticResource Foreground_1_Color}" />
    <SolidColorBrush x:Key="Foreground_2" Color="{StaticResource Foreground_2_Color}" />
    <SolidColorBrush x:Key="Foreground_3" Color="{StaticResource Foreground_3_Color}" />
    <SolidColorBrush x:Key="Foreground_4" Color="{StaticResource Foreground_4_Color}" />
    <SolidColorBrush x:Key="Foreground_5" Color="{StaticResource Foreground_5_Color}" />

    <SolidColorBrush x:Key="Primary_1" Color="{StaticResource Primary_1_Color}" />
    <SolidColorBrush x:Key="Primary_2" Color="{StaticResource Primary_2_Color}" />
    <SolidColorBrush x:Key="Primary_3" Color="{StaticResource Primary_3_Color}" />
    <SolidColorBrush x:Key="Primary_4" Color="{StaticResource Primary_4_Color}" />
    <SolidColorBrush x:Key="Primary_5" Color="{StaticResource Primary_5_Color}" />

    <SolidColorBrush x:Key="Secondary_1" Color="{StaticResource Secondary_1_Color}" />
    <SolidColorBrush x:Key="Secondary_2" Color="{StaticResource Secondary_2_Color}" />
    <SolidColorBrush x:Key="Secondary_3" Color="{StaticResource Secondary_3_Color}" />
    <SolidColorBrush x:Key="Secondary_4" Color="{StaticResource Secondary_4_Color}" />
    <SolidColorBrush x:Key="Secondary_5" Color="{StaticResource Secondary_5_Color}" />

    <SolidColorBrush x:Key="Accent_1" Color="{StaticResource Accent_1_Color}" />
    <SolidColorBrush x:Key="Accent_2" Color="{StaticResource Accent_2_Color}" />
    <SolidColorBrush x:Key="Accent_3" Color="{StaticResource Accent_3_Color}" />
    <SolidColorBrush x:Key="Accent_4" Color="{StaticResource Accent_4_Color}" />
    <SolidColorBrush x:Key="Accent_5" Color="{StaticResource Accent_5_Color}" />

    <SolidColorBrush x:Key="Scroll_1" Color="{StaticResource Scroll_1_Color}" />

    <SolidColorBrush x:Key="GroupContentBackground_1" Color="{StaticResource GroupContentBackground_1_Color}" />
    <SolidColorBrush x:Key="GroupContentBackground_2" Color="{StaticResource GroupContentBackground_2_Color}" />

    <SolidColorBrush x:Key="ListItemSeparator_1" Color="{StaticResource ListItemSeparator_1_Color}" />
    <SolidColorBrush x:Key="ListItemSeparator_2" Color="{StaticResource ListItemSeparator_2_Color}" />
    <!--#endregion-->

    <LinearGradientBrush x:Key="GroupContentBackgroundGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0" Color="{StaticResource GroupContentBackground_1_Color}" />
        <GradientStop Offset="1" Color="{StaticResource GroupContentBackground_2_Color}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ListItemSeparatorGradient" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Offset="0" Color="{StaticResource ListItemSeparator_2_Color}" />
        <GradientStop Offset="0.25" Color="{StaticResource ListItemSeparator_1_Color}" />
        <GradientStop Offset="0.75" Color="{StaticResource ListItemSeparator_1_Color}" />
        <GradientStop Offset="1" Color="{StaticResource ListItemSeparator_2_Color}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PopupBackGroundGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0" Color="{StaticResource PopUp_1_Color}" />
        <GradientStop Offset="1" Color="{StaticResource PopUp_2_Color}" />
    </LinearGradientBrush>


    <!--  Background Colors  -->
    <Color x:Key="primaryBackColor1">#0A003A</Color>
    <Color x:Key="primaryBackColor2">#1C0E49</Color>
    <Color x:Key="secondaryBackColor1">#24105F</Color>
    <Color x:Key="secondaryBackColor2">#210F55</Color>

    <!--  Window Border Colors  -->
    <Color x:Key="winBorderColor1">#6D2FFF</Color>
    <Color x:Key="winBorderColor2">#FB539B</Color>
    <Color x:Key="winBorderColor3">#836EFB</Color>

    <!--  Color Palette  -->
    <SolidColorBrush x:Key="color1" Color="#E285DE" />
    <SolidColorBrush x:Key="color2" Color="#784DFD" />
    <SolidColorBrush x:Key="color3" Color="#4ADAEC" />
    <SolidColorBrush x:Key="color4" Color="#FB539B" />
    <SolidColorBrush x:Key="color5" Color="#7E82FC" />
    <SolidColorBrush x:Key="color6" Color="#FFC047" />
    <SolidColorBrush x:Key="color7" Color="#EF6C96" />
    <SolidColorBrush x:Key="color8" Color="#78A3FC" />
    <SolidColorBrush x:Key="color9" Color="#07F3C0" />
    <SolidColorBrush x:Key="color10" Color="#FBA1AA" />

    <!--  Font Colors  -->
    <SolidColorBrush x:Key="titleColor1" Color="#E0E1F1" />
    <SolidColorBrush x:Key="titleColor2" Color="#D5CFF5" />
    <SolidColorBrush x:Key="titleColor3" Color="#BCBEE0" />
    <SolidColorBrush x:Key="plainTextColor1" Color="#9497CD" />
    <SolidColorBrush x:Key="plainTextColor2" Color="#7C80C2" />
    <SolidColorBrush x:Key="plainTextColor3" Color="#7376BD" />

    <!--  Panel Colors  -->
    <SolidColorBrush x:Key="panelColor" Color="#200F53" />
    <SolidColorBrush x:Key="panelOverColor" Color="#281269" />
    <SolidColorBrush x:Key="panelActiveColor" Color="#2B1372" />

    <!--  Button Colors  -->
    <!--  TextBox Colors  -->
    <!--  Etc  -->

</ResourceDictionary>